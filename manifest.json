{
    "manifest_version": 3,
    "name": "SPb Weather",
    "version": "1.1",
    "description": "Displays the current weather in Saint Petersburg.",
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/default.png"
    },
    "icons": {
      "16": "icons/default.png",
      "48": "icons/default.png",
      "128": "icons/default.png"
    }
  }